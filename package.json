  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "server.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "ts-node-dev ./src/server.ts",
    "test": "npx mocha -r ts-node/register 'test/*.test.ts' --file test/mocha-setup.ts",
    "lint": "npx eslint \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\" \"test/*.test.ts\" --quiet --fix",
    "format": "npx prettier \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\" --write",
    "check": "npm run lint & npm run format"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.fdmci.hva.nl/ad-software-development-2324/fdmci-ict-ad-sd101/sem2/vloot/backend.git"
  },
