    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.3.1",
    "eslint": "^7.32.0",
    "mocha": "^9.2.2",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.9",
    "supertest": "^6.1.6",
    "ts-node-dev": "^2.0.0",
    "ts-sinon": "^2.0.2",
    "typescript": "^5.1.6"
